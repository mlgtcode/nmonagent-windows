{
  "name": "nmonagent-windows-2",
  "version": "2.0.0",
  "description": "nMon Windows Agent",
  "main": "main.js",
  "bin": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "pkg . --targets node18-win-x64 --output dist/nmonagent.exe",
    "build:all": "pkg . --targets node18-win-x64,node18-linux-x64,node18-macos-x64 --out-path dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codeniner/nmonagent-windows-2.git"
  },
  "author": "Codeniner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/codeniner/nmonagent-windows-2/issues"
  },
  "homepage": "https://github.com/codeniner/nmonagent-windows-2#readme",
  "dependencies": {
    "axios": "^1.13.2",
    "fs": "0.0.1-security",
    "os": "^0.1.2",
    "os-service": "^2.2.0",
    "path": "^0.12.7",
    "systeminformation": "^5.30.5",
    "url": "^0.11.4"
  },
  "devDependencies": {
    "pkg": "^5.8.1"
  },
  "pkg": {
    "scripts": [
      "main.js",
      "service.js"
    ],
    "assets": [
      "icon.ico",
      "configure.cmd",
      "remove.cmd"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  }
}
